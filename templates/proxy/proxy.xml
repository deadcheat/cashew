<cas:serviceResponse xmlns:cas='http://www.yale.edu/tp/cas'>{{ if .ProxySuccess }}
    <cas:proxySuccess>
        <cas:proxyTicket>{{ .ProxyTicket }}</cas:proxyTicket>
    </cas:proxySuccess>{{ else }}
    <cas:proxyFailure code="{{ .ErrorCode }}">
        {{ .ErrorBody }}
    </cas:proxyFailure>{{ end }}
</cas:serviceResponse>
